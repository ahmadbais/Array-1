#include <iostream>
using namespace std;
int nilai[5];
int i, hasil=0;
int main ()
  {
  	for (int i=0;i<5; ++i)
  {
     cout<<"Input Nilai Ke - "<<(i+1)<<" : ";
     cin>>nilai[i];
  }
  cout<<endl;
  for (int i=0;i<5; ++i)
  {
     hasil += nilai [i];
     cout<<"Hasil Ke - "<<i<<" Adalah : "<<hasil<<endl;
  }
  cout<<endl<<" Jumlah Keseluruhan Umum : "<<hasil<<endl;
  return 0;
  }
